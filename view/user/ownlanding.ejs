    <title>Bankai - Shop</title>
    <style>
        html, body {
            height: 100%; /* Set full height */
            margin: 0; /* Remove default margin */
            overflow-x: hidden; /* Prevent horizontal overflow */
        }

        body {
            font-family: Arial, sans-serif;
            display: flex;
            flex-direction: column; /* Allows for flex column layout */
        }

        .banner {
            background-color: #17a2b8; /* Info color */
        }

        .content {
            flex: 1; /* Allow content to grow and fill the available space */
            display: flex; /* Use flex for centering */
            justify-content: center; /* Center horizontally */
            align-items: center; /* Center vertically */
            flex-direction: column; /* Stack elements vertically */
            text-align: center; /* Center text */
            padding: 20px; /* Padding for content */
        }

        .image-container {
            width: 80%; /* Adjust width as needed */
            max-width: 800px; /* Max width for larger screens */
            height: auto; /* Auto height */
        }

        .product-container {
            display: flex; /* Flexbox for product display */
            justify-content: space-around; /* Space around products */
            margin-top: 30px; /* Space above products */
        }

        .product {
            width: 45%; /* Width for each product */
            text-align: center; /* Center text for products */
        }

        footer {
            background-color: #f8f9fa; /* Light color */
            position: relative;
            bottom: 0;
            width: 100%;
        }

        .footer-text {
            color: #6c757d; /* Bootstrap's muted color */
            margin-bottom: 0; /* Remove bottom margin */
        }
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Arial, sans-serif;
        }

        .container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 20px;
        }

        h1 {
            text-align: center;
            margin-bottom: 40px;
            font-size: 32px;
            letter-spacing: 2px;
        }

        .products-grid {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
            gap: 30px;
        }

        .product-card {
            border: 1px solid #e0e0e0;
            border-radius: 8px;
            padding: 20px;
            text-align: center;
            transition: transform 0.2s;
        }

        .product-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 5px 15px rgba(0,0,0,0.1);
        }

        .product-image {
            width: 100%;
            height: 200px;
            object-fit: contain;
            margin-bottom: 15px;
        }

        .product-title {
            font-size: 14px;
            margin-bottom: 10px;
            color: #333;
            text-transform: uppercase;
        }

        .product-price {
            font-weight: bold;
            color: #000;
            font-size: 16px;
        }

        .shop-all-btn {
            display: block;
            width: 200px;
            margin: 40px auto;
            padding: 15px 30px;
            background-color: #333;
            color: white;
            text-align: center;
            text-decoration: none;
            border-radius: 5px;
            transition: background-color 0.2s;
        }

        .shop-all-btn:hover {
            background-color: #555;
        }

        @media (max-width: 768px) {
            .products-grid {
                grid-template-columns: repeat(2, 1fr);
            }
        }

        @media (max-width: 480px) {
            .products-grid {
                grid-template-columns: 1fr;
            }
        }
        

        /* {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Arial, sans-serif;
        } */

        footer {
            background-color: #f9f9f9;
            padding: 60px 0;
            color: #333;
            border-top: 1px solid #eee;
        }

        .footer-container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 20px;
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            gap: 40px;
        }

        .footer-section h3 {
            font-size: 18px;
            font-weight: 700;
            margin-bottom: 20px;
            text-transform: uppercase;
            color: #444;
        }

        .footer-section p {
            font-size: 14px;
            line-height: 1.8;
            color: #666;
            margin-bottom: 20px;
        }

        .footer-section ul {
            list-style: none;
        }

        .footer-section ul li {
            margin-bottom: 12px;
        }

        .footer-section ul li a {
            color: #666;
            text-decoration: none;
            font-size: 14px;
            transition: color 0.3s ease;
        }

        .footer-section ul li a:hover {
            color: #007BFF;
        }

        .social-icons {
            display: flex;
            gap: 15px;
            margin-top: 20px;
        }

        .social-icons a {
            color: #666;
            text-decoration: none;
            font-size: 18px;
            transition: color 0.3s ease;
        }

        .social-icons a:hover {
            color: #007BFF;
        }

        .contact-info p {
            margin-bottom: 10px;
            font-size: 14px;
            line-height: 1.6;
            color: #444;
        }

        .contact-info a {
            color: #007BFF;
            text-decoration: none;
        }

        .footer-bottom {
            margin-top: 40px;
            text-align: center;
            padding-top: 20px;
            border-top: 1px solid #eee;
            font-size: 12px;
            color: #666;
        }

        .footer-bottom a {
            color: #007BFF;
            text-decoration: none;
        }

        .footer-bottom .heart {
            color: #ff4444;
        }

        @media (max-width: 1024px) {
            .footer-container {
                grid-template-columns: repeat(3, 1fr);
            }
        }

        @media (max-width: 768px) {
            .footer-container {
                grid-template-columns: repeat(2, 1fr);
            }
        }

        @media (max-width: 480px) {
            .footer-container {
                grid-template-columns: 1fr;
            }
        }
    </style>
</head>

