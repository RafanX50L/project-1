								</div>
								<div class="button-group">
									<button class="icon-button add-to-cart-btn" data-product-id="<%= item.productId._id %>">
										<i class="fa fa-shopping-cart"></i>
									</button>
									<button class="icon-button remove-button" data-product-id="<%= item.productId._id %>">
										<i class="fa fa-trash"></i>
									</button>
								</div>
							</div>
						</div>
					<% }) %>
				<% } %>
			</div>
		</div>
	</section>
    

